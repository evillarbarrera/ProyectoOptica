<%= form_with(model: company, local: true) do |form| %>
<div class="form-style-9">
  <% if company.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(company.errors.count, "error") %> prohibited this company from being saved:</h2>

      <ul>
      <% company.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>



<ul>
  <P1>
  <li >   
    <%= form.text_field :Nombre ,class:"field-style field-split align-left" ,placeholder:"Nombre"%>
    <%= form.text_field :Domicilio ,class:"field-style field-split align-right" ,placeholder:"Domicilio"%>
  </li>
<li >   
    <%= form.text_field :Pais  ,class:"field-style field-split align-left" ,placeholder:"Marca"%>
    <%= form.text_field :Telefono ,class:"field-style field-split align-right" ,placeholder:"Telefono"%>
  </li>
  <li >   
    <%= form.text_field :Email  ,class:"field-style field-split align-left" ,placeholder:"Email"%>
    <%= form.text_field :Web ,class:"field-style field-split align-right" ,placeholder:"Web"%>
  </li>


  <!--<div class="field">
    <%= form.label :vigente %>
    <%= form.number_field :vigente %>
  </div>-->

  <li class="actions">
    <%= form.submit %>
  </li>
<% end %>

</ul>
</form>
