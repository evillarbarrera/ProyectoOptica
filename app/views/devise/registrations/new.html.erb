<div  class="form-style-barra">
CREAR USUARIO
</div>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

<div class="form-style-9">
<ul>
  <li>
   
    <%= f.text_field :nombre, autofocus: true ,class:"field-style field-split align-left" ,placeholder:"Nombre"%>
     <%= f.text_field :email, autofocus: true ,class:"field-style field-split align-right" ,placeholder:"Email"%>
  </li>

<li>
  
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    
      <%= f.password_field :password, autocomplete: "off" , class:"field-style field-split align-left" , placeholder:"Contrasena"%>

      <%= f.password_field :password_confirmation, autocomplete: "off" , class:"field-style field-split align-right" , placeholder:"Confirmar Contrasena"%>
  </li>
<br>
  <li>
      <%= f.select :user_types_id, options_from_collection_for_select( UserType.all, :id, :Nombre) , class:"field-style field-split align-right"%>
  </li>




  <div class="actions">
    <%= f.submit "Ingresar Usuario" %>
  </div>

<%= render "devise/shared/links" %>

<%end%>
</ul>
</div>
</form>

